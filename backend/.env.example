### Example env for backend (DO NOT commit your real .env with secrets)
# Copy this file to .env and fill values locally on your machine

# Ollama integration (optional)
# For local Ollama (no API key required): set OLLAMA_URL to http://localhost:11434
OLLAMA_URL=http://localhost:11434
# If your Ollama service requires an API key, set it here locally (do NOT commit)
OLLAMA_API_KEY=
OLLAMA_MODEL=mistral

# Other env vars used by the backend (examples)
MONGO_URI=
PORT=5001
